{
  //address∫Õport≈‰÷√
  "Http": {
    "ssl": "false",
    "port": "8080"
  },
  "JWT": {
    "SecurityKey": "tv6hZmIqf5xlStMt",
    "Issuer": "http://localhost:5200",
    "Audience": "http://localhost:5200",
    "ExpiresMinutes": 20160
  },
  //Orleans≈‰÷√
  "Orleans": {
    "ClusterId": "Cluster",
    "ServiceId": "Service",
    "HostName": "127.0.0.1",
    "SiloPort": 11111,
    "GatewayPort": 30000,
    "Database": "orleans",
    "Connection": "mongodb://localhost:27017/"
  },
  "WeChat": {
    "AppId": "this is AppId",
    "AppSecret": "this is AppSecret"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Process"
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            { "Name": "Console" },
            {
              "Name": "MongoDB",
              "Args": {
                "databaseUrl": "mongodb://localhost:27017/logs",
                "collectionName": "api"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "logs/console.log",
                "rollingInterval": "Day"
              }
            }
          ]
        }
      }
    ]
  }
}